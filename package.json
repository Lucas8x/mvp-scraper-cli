{
  "name": "mvp-scraper",
  "version": "0.1.0",
  "description": "cli for extracting information about ragnarok mvp monsters",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node --no-warnings dist/src/index.js",
    "cli": "tsx watch src/index.ts",
    "prepack": "npm run build",
    "build": "npx tsc"
  },
  "bin": {
    "mvp-scrap": "./dist/src/index.js"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucas8x/mvp-scraper-cli"
  },
  "bugs": {
    "url": "https://github.com/Lucas8x/mvp-scraper-cli/issues"
  },
  "homepage": "https://github.com/Lucas8x/mvp-scraper-cli#readme",
  "author": "lucas8x",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "divine-pride-api-wrapper": "^0.1.11",
    "nanospinner": "^1.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/prompts": "^2.4.4",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "ragnarok",
    "divine",
    "pride",
    "mvp"
  ]
}
